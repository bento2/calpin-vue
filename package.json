    "uuid": "^13.0.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.10.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
